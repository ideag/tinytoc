msgid "What format to use for auto generated TOC links."
msgstr "Wybierz format automatycznie generowanych odnośników spisu treści."

#: tinytoc.php:112
msgid "Numeric (e.g. #h1, #h2 ...)"
msgstr "Numeryczny (np. #h1, #h2 ...)"

#: tinytoc.php:113
msgid "Text (e.g. #my-title-here)"
msgstr "Tekstowy (np. #moj-naglowek)"

#: tinytoc.php:114
msgid "Combined (e.g. #h1-my-title-here)"
msgstr "Mieszany (np. #h1-moj-naglowek)"

